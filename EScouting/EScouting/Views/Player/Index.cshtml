@model IEnumerable<EScouting.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>all Players</h2>

@foreach(var player in Model)
{
    <p>@Html.ActionLink(player.Name,"Details", "Player", new { id = player.Id}, null)</p>
    <p>@player.EPoints</p>
}